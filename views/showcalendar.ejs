<body class="skin-blue">
 <% include header.ejs %>
  <div class="wrapper row-offcanvas row-offcanvas-left">
  <% include leftside.ejs %>
  <aside class="right-side"> 
<main class="procedure"> 
<div id="content" class="site-content"> 
<div class="legal">
<div class="page-section legal-content">
<div class="container" id="myAppBody" ng-app="calendarAngularApp" ng-controller="calendarViewController as calenderView">
<% if(isAdmin){ %>
<script>

	ifUserAdmin = 1;
		
</script>
<% } %>
	<div id ="chooseOtherUserCalanderDiv" class="row">
		<div class="col-md-3" id="selectUserText">Select User </div>
		<div class="col-md-3">    
          <select     ng-model="calenderView.userSelection" id="selectUserInput"  ng-options="user.firstName for user in calenderView.userList" ></select>
        </div>
		
	</div>
	<div class="row">
		
		<div class="col-md-10" id='calendar121'></div>
	</div>
	
<div id="addEventModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
 <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;  </button>
        <h4 class="modal-title" id="myModalLabel">Add Event</h4>
      </div>
      <div class="modal-body" id="assetUploadModal">
		  <div  id="eventNameText">Event name</div>
        <input type="text" class="form-control" id="eventNameTextInput" maxlength="100">
			<div style="clear:both;"></div>
        <form name="assetUpload_form" id="assetUpload_form" enctype="multipart/form-data" method="post" action="assetUploadScreen.php"> <div id="assetImgUploadDiv"><div  id="assetImgUploadLabel">Asset upload</div><label id="assetImgBrowseBtn" class="btn btn-primary"><input type="file" id="assetImgFileInput" name="assetImgFileInput">Choose file</label><div id="assetImgUploadBtn"  class="btn btn-primary">Upload</div><div id="assetBrowsedFileName"></div><div id="assetuploader"></div></div></form>
        
        
      </div>
      <div class="modal-footer">
		  <span class="pull-left" id="saveAssetWarnText"></span>
        <button type="button" id = "addAssetModalCloseBtn" class="btn btn-default" data-dismiss="modal">Close</button>
        <button type="button" id = "addAssetModalSaveBtn" class="btn btn-primary">Save changes</button>
		<button type="button" id = "addAssetModalDeleteBtn" class="btn btn-danger">Delete</button>
      </div>
    </div>
  </div>
</div>


<div id="changeEventModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
 <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" id = "changeEventModalCloseBtn" class="close" data-dismiss="modal" aria-hidden="true">&times;  </button>
        <h4 class="modal-title" id="myModalLabel">Change Event</h4>
      </div>
      <div class="modal-body" id="changeEventTextBox">
		  <div  id="eventNameText">Are you sure about this change?</div>     
        
      </div>
      <div class="modal-footer">		  
        
        <button type="button" id = "changeEventYesBtn" class="btn btn-primary">Yes</button>
		<button type="button" id = "changeEventNoBtn" class="btn btn-primary">No</button>
      </div>
    </div>
  </div>
</div>
	
	
</div><!-- end container -->
</div><!-- .page-section legal-content -->
